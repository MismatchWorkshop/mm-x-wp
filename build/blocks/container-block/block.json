{
  "apiVersion": 3,
  "name": "wagepoint/container",
  "title": "Container",
  "category": "layout",
  "icon": "align-center",
  "description": "A flexible container with background and spacing controls",
  "attributes": {
    "outerBackground": {
      "type": "string",
      "default": "light"
    },
    "spacing": {
      "type": "string",
      "default": "normal"
    },
    "width": {
      "type": "string",
      "default": "normal"
    },
    "containerBackground": {
      "type": "string",
      "default": "transparent"
    },
    "padding": {
      "type": "string",
      "default": "normal"
    },
    "contentWidth": {
      "type": "string",
      "default": "full"
    },
    "textAlignment": {
      "type": "string",
      "default": "left"
    }
  },
  "providesContext": {
    "wagepoint/containerBackground": "containerBackground"
  },
  "supports": {
    "html": false
  },
  "textdomain": "wagepoint",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}