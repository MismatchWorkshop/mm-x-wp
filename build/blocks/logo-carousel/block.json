{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wagepoint/logo-carousel",
  "title": "Logo Carousel",
  "category": "widgets",
  "icon": "images-alt2",
  "description": "Display client logos with smart background system.",
  "supports": {
    "html": false,
    "inserter": false
  },
  "attributes": {
    "logos": {
      "type": "array",
      "default": []
    },
    "heading": {
      "type": "string",
      "default": "Trusted by 30,000+ businesses"
    },
    "containerBackground": {
      "type": "string",
      "default": "transparent"
    },
    "headingTextColor": {
      "type": "string",
      "default": "auto"
    },
    "headingLevel": {
      "type": "number",
      "default": 2
    }
  },
  "textdomain": "wagepoint",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}